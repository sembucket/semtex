<FIELDS>
	u v w p
</FIELDS>

<USER>
	u = 0.0
	v = 0.0
	w = 0.0
#w=tanh(BLT*(R_w/(eps+abs(y-R_c))-abs(y-R_c)/R_w))*tanh(BLT*(H_h/(eps+abs(x-H_c))-abs(x-H_c)/H_h))*(heav(x-x_in1)-heav(x-x_in2))*(heav(y-R_i+eps)-heav(y-R_o)-eps)
	p = 0.0
        MASK_LAG (2./N_Z)+tanh(8.*y)
</USER>

<TOKENS>

        MASK_DELTA  = 4
        MASK_ORDER  = 4

	CYLINDRICAL = 1
	CHKPOINT    = 1
	AVERAGE	    = 0
	ITERATIVE   = 0
	D_T       = 0.0001
	TOL_REL   = 1.0e-8
	KINVIS    = 1.0e-3
	N_TIME    = 2
	N_P    = 9
	N_Z       = 64
	BETA      = 1.0
	N_STEP    = 200
	IO_CFL    = 10
	IO_FLD    = 50
	U_CL	  = 1.0
	R_i       = 0.095
	R_o       = 0.195
	W_in      = 0.082
	theta1    = asin((R_o-W_in)/R_o)
	theta2    = PI/2.0
	x_in1     = 0.375
	x_in2     = 0.580
	H_in	  = x_in2-x_in1
	W_h       = 0.5*W_in
	H_h       = 0.5*H_in
	W_c	  = R_o-W_h
	H_c	  = 0.5*(x_in2+x_in1)
	BLT       = 2.0
	eps	  = 1.0e-6
	R_c       = 0.5*(R_i+R_o)
	R_w       = R_o-R_i
</TOKENS>

#        REFVIS    = 20.0*KINVIS
#        RNG       = 0
#        C_SMAG    = 0.095

