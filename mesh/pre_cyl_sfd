# RM92
# D=90 mm H=120 mm  h = 2.667
#   (k,l) = (1,1) omega' = 2.64298 (Table 1)
#   translates to IRPHE notation omega = 2/omega' = 0.757, agrees with dispersion_relation.py
# can axial -0.5 .. 0.5
#     radial   0 .. 0.5
# use x_scale
##############################################################################

<TOKENS>
	CYLINDRICAL = 1
	BETA        = 1.0
	X_SCALE     = 1.3335

	THETA_DEG = 3.
	THETA_MAX   = THETA_DEG/DEG
	Re        = 6500.
	omega       = 2/2.72
	OMEGA       = 1.0

	OMEGA1      = omega
	OMEGA2      = (1.0-omega)/cos(THETA_MAX)

	KINVIS      = 0.25/Re

	PERIOD      = TWOPI/OMEGA1
	T_RISE      = 0.1*PERIOD
	DTH_DT      = THETA_MAX/T_RISE

	STEPS_P   = 1024.*4.*THETA_DEG
	D_T         = PERIOD/STEPS_P
	N_STEP      = (2100.+300*3./THETA_DEG)/D_T
	N_TIME      = 2
	N_P         = 7
	N_Z         = 128

	IO_HIS      = 32
	IO_CFL      = 3200
	IO_FLD      = STEPS_P/20.

	AVERAGE     = 0
	ITERATIVE   = 0
	CHKPOINT    = 1

	TOL_REL     = 1e-8
        MDL_ROT_OMEGA = OMEGA1

        SFD_PERIOD = 50.
        SFD_T1 = 2000.
</TOKENS>


<FORCE>
	CORIOLIS_UNSTEADY = 1

	CORIOLIS_OMEGA_X = OMEGA2*cos(DTH_DT*(t-(t-T_RISE)*heav(t-T_RISE)))
	CORIOLIS_OMEGA_Y = 0.0
	CORIOLIS_OMEGA_Z = OMEGA2*sin(DTH_DT*(t-(t-T_RISE)*heav(t-T_RISE)))

	CORIOLIS_DOMEGA_X_DT = -OMEGA2*DTH_DT*(1.0-heav(t-T_RISE))*sin(DTH_DT*t*(1-heav(t-T_RISE)))
	CORIOLIS_DOMEGA_Y_DT = 0.0
	CORIOLIS_DOMEGA_Z_DT =  OMEGA2*DTH_DT*(1.0-heav(t-T_RISE))*cos(DTH_DT*t*(1-heav(t-T_RISE)))

        SFD_DELTA = SFD_PERIOD/TWOPI
        SFD_CHI = 0.1
</FORCE>

<USER>
	u = 0
	v = 0
	w = OMEGA1*y
	p = 0
</USER>

<FIELDS>
	 u v w p
</FIELDS>

