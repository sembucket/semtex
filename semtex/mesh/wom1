# 4 element womersley flow 

<FIELDS NUMBER=3>
	u v p
</FIELDS>

<TOKENS>
	CYLINDRICAL = 1
	N_TIME      = 2
	N_P      = 9
	N_STEP      = 2500
	D_T         = 0.001
	KINVIS      = 0.01
        IO_FLD      = 50
        IO_CFL      = 20
        IO_HIS      = 20
	CHKPOINT    = 0
	PipeR       = 0.5
	Wnum        = 2*PI/2.5
</TOKENS>

<USER>
u = womcos(y,PipeR,KINVIS,0,t)+0.75*womsin(y,PipeR,KINVIS,Wnum,t)
v = 0
p = 0
</USER>

<GROUPS NUMBER=4>
	1	v	value
	2	w	wall
	3	o	exit
	4	a	axis
</GROUPS>


<HISTORY NUMBER=4>
	1   	0 	0	0
	2	0.5	0	0
	3	1.0	0	0
	4	3	0	0
</HISTORY>

<BCS NUMBER=4>
	1	v	3
