			<D>	v = 0.0				</D>
			<H>	p = 0				</H>
	2	w	3
			<D>	u = 0.0				</D>
			<D>	v = 0.0				</D>
			<H>	p = 0				</H>
	3	o	3
			<N>	u = 0.0				</N>
			<N>	v = 0.0				</N>
			<D>	p = 0.0				</D>
	4	a	3
			<A>	u 				</A>
			<A>	v 				</A>
			<A>	p 				</A>
</BCS>

